package t1;

public class SpecialInt{
    	public int i;
    	public long l;
    	public double d;
    	public char c;
    	
    	public SpecialInt(int i) {
    		this.i = i;
    		l = 5L;
    		d = 0.2;
    		c = 'a';
    	}
    	
    	public String toString() {
    		return " i: " + i + " l: " + l + " d: " + d + " c: " + c;
    	}
    }